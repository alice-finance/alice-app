{
  "expo": {
    "name": "Alice",
    "slug": "alice-finance",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.3",
    "extra": {
      "pubVersion": 1
    },
    "orientation": "portrait",
    "primaryColor": "#8315d6",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "finance.alice.app",
      "buildNumber": "10"
    },
    "android": {
      "icon": "./src/assets/icon.android.png",
      "package": "finance.alice.app",
      "permissions": [],
      "versionCode": 10
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {}
        }
      ]
    }
  }
}
